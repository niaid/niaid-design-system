<!DOCTYPE html>
<html lang="{{ language|default('en') }}" class="{{ htmlClass }}">
  <head>
    <title>{{ title }}</title>
    {% if metaDescription is not empty %}<meta name="description" content="{{ metaDescription }}">{% endif %}
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width" />
    {% if cssIncludes.bootstrapDatepicker %}<link rel="stylesheet" href="{{ cssPath }}libraries/bootstrap-datepicker/bootstrap-datepicker.min.css" />{% endif %}
    {% if cssIncludes.select2 %}<link rel="stylesheet" href="{{ cssPath }}libraries/select2/select2.min.css" />{% endif %}
    {% if cssIncludes.uswds %}<link rel="stylesheet" href="{{ cssPath }}libraries/uswds/uswds-banner.min.css" />{% endif %}
    
    <link rel="stylesheet" href="{{ cssPath }}custom/prism/prism.css" />

    <link rel="icon" href="{{ imagePath }}global/favicons/favicon.ico" type="image/x-icon"/>

    {% if prodBuild != "true" %}
      <link rel="stylesheet" href="{{ cssPath }}nds-min.css?{{ cacheBuster }}" media="all" />
    {% else %}
      <link rel="stylesheet" href="{{ cssPath }}nds-min.css" media="all" />
    {% endif %}
    {% if pageID == "builder" %}
      <link rel="stylesheet" href="{{ cssPath }}builder.css" media="all" />
    {% endif %}

    {% if prodBuild != "true" %}<link rel="stylesheet" href="{{ cssPath }}libraries/pattern-lab/pattern-scaffolding.css?{{ cacheBuster }}" media="all" />{% endif %}

    {% if prodBuild != "true" %}
      <!-- Begin Pattern Lab (Required for Pattern Lab to run properly) -->
      {{ patternLabHead | raw }}
      <!-- End Pattern Lab -->
    {% endif %}

    {% if prodBuild == "true" %}
      <script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
      new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
      j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
      'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
      })(window,document,'script','dataLayer','GTM-56JHMG');</script>
    {% endif %}
  </head>
  <body class="{{ bodyClass }}">
    <!-- Google Tag Manager (noscript) -->
    <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-56JHMG"
    height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) -->