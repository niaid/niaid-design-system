{% set media_video_youtube_classes = media_video_youtube_classes|default() %}
{% set media_video_youtube_id = media_video_youtube_id|default() %}
{% set media_video_youtube_title = media_video_youtube_title|default() %}
{% set media_video_youtube_src = media_video_youtube_src|default() %}
{% set media_video_youtube_height = media_video_youtube_height|default() %}
{% set media_video_youtube_width = media_video_youtube_width|default() %}
{% set media_video_youtube_attributes = media_video_youtube_attributes|default() %}

<div class="media-nds media--video-youtube {{ media_video_youtube_classes }}"{% if media_video_youtube_id is not empty %} id="{{ media_video_youtube_id }}"{% endif %}>
    <iframe class="media--video-youtube__iframe" allowfullscreen="" frameborder="0" height="{{ media_video_youtube_height }}" width="{{ media_video_youtube_width }}" src="{{ media_video_youtube_src }}" title="{{ media_video_youtube_title }}" {{ media_video_youtube_attributes }}></iframe>
</div>